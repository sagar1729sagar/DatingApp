            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_margin="30dp"
                android:orientation="horizontal"
                android:layout_below="@id/contact_button"
                >

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/like_icon"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:materialIcon="thumb_up_outline"
                    app:materialIconColor="@color/leaf_green" />

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/stact_chat_icon"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    app:materialIcon="comment_processing_outline"
                    app:materialIconColor="@color/leaf_green" />

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/favourite_add_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    app:materialIcon="star"
                    app:materialIconColor="@color/leaf_green" />

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/add_friend_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    app:materialIcon="account_multiple"
                    app:materialIconColor="@color/leaf_green" />

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/report_user_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    app:materialIcon="alert"
                    app:materialIconColor="#e9e10e" />

                <net.steamcrafted.materialiconlib.MaterialIconView
                    android:id="@+id/block_user_icon"
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginStart="10dp"
                    app:materialIcon="block_helper"
                    app:materialIconColor="@android:color/holo_red_dark" />
            </LinearLayout>


        </RelativeLayout>
    </ScrollView>
